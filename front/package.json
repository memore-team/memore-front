{
  "name": "memore",
  "version": "1.0.0",
  "description": "The simple memo application",
  "main": "index.js",
  "engines": {
    "node": "6.8.1"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm-run-all -p start watch",
    "start": "browser-sync start --server './'",
    "watch": "watchify -v -d src/app.jsx --extension=jsx -t babelify -p livereactload -p [css-modulesify --after autoprefixer --autoprefixer.browsers 'last 2 versions' -o dist/bundle.css] -o dist/bundle.js",
    "build": "browserify src/app.jsx --extension=jsx -t babelify -p [css-modulesify --after autoprefixer --autoprefixer.browsers 'last 2 versions' --after cssnano -o dist/bundle.css] -g uglifyify -o dist/bundle.js",
    "lint": "eslint --ext .js,.jsx ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabeliwo/memore.git"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabeliwo/memore/issues"
  },
  "homepage": "https://github.com/nabeliwo/memore#readme",
  "dependencies": {
    "flux": "^3.1.0",
    "npm-run-all": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.3",
    "css-modulesify": "^0.25.1",
    "cssnano": "^3.7.7",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "livereactload": "^3.0.1",
    "react-proxy": "^1.1.8",
    "uglifyify": "^3.0.4",
    "watchify": "^3.7.0"
  }
}
