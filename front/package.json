{
  "name": "memore",
  "version": "1.0.0",
  "description": "The simple memo application",
  "main": "index.js",
  "engines": {
    "node": "6.8.1"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "npm-run-all -p start watch",
    "start": "browser-sync start --server './'",
    "watch": "watchify src/app.jsx --extension jsx -v -t babelify -g envify -p livereactload -p [css-modulesify -o dist/bundle.css] -o dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nabeliwo/memore.git"
  },
  "author": "nabeliwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nabeliwo/memore/issues"
  },
  "homepage": "https://github.com/nabeliwo/memore#readme",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.3",
    "css-modulesify": "^0.25.1",
    "envify": "^3.4.1",
    "livereactload": "^3.0.1",
    "npm-run-all": "^3.1.1",
    "react-proxy": "^1.1.8",
    "watchify": "^3.7.0"
  }
}
