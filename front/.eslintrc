{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "react/forbid-prop-types": ['error', { forbid: ['any'] }]
  },
  "plugins": ["react"],
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
